<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:TeamsWP.Controls"
    xmlns:local="using:TeamsWP.Styles">

  <Style TargetType="controls:TeamsImage">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="controls:TeamsImage">
          <Canvas>
            <Image x:Name="Image" />
            <Canvas Width="100" Height="100" x:Name="RenderCanvas" Margin="1000"/>
          </Canvas>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>
